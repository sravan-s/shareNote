<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.qasas.sharenote"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="14"
        android:targetSdkVersion="22" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.qasas.sharenote.universityAndCourseSelector"
            android:label="@string/app_name" >
        </activity>
        <activity
            android:name="com.example.qasas.sharenote.universityInfo"
            android:label="@string/title_activity_university_info"
            android:parentActivityName="com.example.qasas.sharenote.universityAndCourseSelector" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.qasas.sharenote.universityAndCourseSelector" />
        </activity>
        <activity
            android:name="com.example.qasas.sharenote.semesterList"
            android:label="@string/title_activity_semester_list"
            android:parentActivityName="com.example.qasas.sharenote.universityInfo" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.qasas.sharenote.universityInfo" />
        </activity>
        <activity
            android:name="com.example.qasas.sharenote.notesList"
            android:label="@string/title_activity_notesl" >
        </activity>
        <activity
            android:name="com.example.qasas.sharenote.LauncherActivicty"
            android:label="@string/title_activity_launcher_activicty" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>